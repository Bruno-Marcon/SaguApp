{
  "expo": {
    "name": "SaguIFC",
    "slug": "SaguIFC",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo-sagu-mobile-brancoBg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundlerIdentifier": "com.saguifc.ios",
      "bundleIdentifier": "com.brunomarcon.SaguIFC",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo-ifc.png",
        "backgroundColor": "#ffffff",
        "package": "com.saguifc.android"
      },
      "package": "com.brunomarcon.SaguIFC"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/logo-ifc.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
