{
  "expo": {
    "name": "SaguIFC",
    "slug": "SaguIFC",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo-ifc.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundlerIdentifier": "com.saguifc.ios",
      "bundleIdentifier": "com.brunomarcon.SaguIFC",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo-ifc.png",
        "backgroundColor": "#ffffff",
        "package": "com.saguifc.android"
      },
      "package": "com.brunomarcon.SaguIFC"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/logo-ifc.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.646655451709-2nn598licuungg35iu7acb8a0upkitvm"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiUrl": "https://sagu-api-production.up.railway.app",
      "apiKey": "c2c783dda516ddaf2d9f2326e05c63946561382a8ce4e22d838",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "53c55cb2-e186-456a-9291-8cfb1177e419"
      }
    }
  }
}